{
    "version": 2,
    "builds": [
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        },
         {
            "src": "styles/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}